#!/usr/bin/env zsh

# Global profile
[ -f /etc/zsh/zprofile ] && . /etc/zsh/zprofile

# Profile
. ~/.config/shell/profile.sh

# GUI Profile
[ -f ~/.config/shell/profile-gui.sh ] && . ~/.config/shell/profile-gui.sh

# GUI Arch Profile
[ -f ~/.config/shell/profile-arch-gui.sh ] && . ~/.config/shell/profile-arch-gui.sh